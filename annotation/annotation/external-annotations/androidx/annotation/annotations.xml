<root>
    <item name="androidx.annotation.VisibleForTesting int otherwise()">
        <annotation name="androidx.annotation.IntDef">
            <val name="value" val="{androidx.annotation.VisibleForTesting.PRIVATE, androidx.annotation.VisibleForTesting.PACKAGE_PRIVATE, androidx.annotation.VisibleForTesting.PROTECTED, androidx.annotation.VisibleForTesting.NONE}" />
        </annotation>
    </item>
    <item name="androidx.annotation.Dimension int unit()">
        <annotation name="androidx.annotation.IntDef">
            <val name="value" val="{androidx.annotation.Dimension.DP, androidx.annotation.Dimension.PX, androidx.annotation.Dimension.SP}" />
        </annotation>
    </item>
</root>
